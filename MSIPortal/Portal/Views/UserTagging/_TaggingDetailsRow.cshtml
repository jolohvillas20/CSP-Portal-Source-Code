<script>
        $(function () {
            $('.btnRemoveItem').click(function () {

                //var Ticket = new Object();
                $row = $(this).closest('tr.item-row');

                $id = $row.find('.id').text();

                $.ajax({
                    url: "/UserTagging/DeleteTagging",
                    type: "POST",
                    data: "{'taggedid' : '" + $id + "'}",
                    contentType: "application/json; charset=utf-8",
                    success: function (r) {

                        window.location.href = "/UserTagging/Tagging/"+@Model.UserAccessHeader.UserID;
                       
                    }
                });


            });

        }); //  arrival


    </script>
